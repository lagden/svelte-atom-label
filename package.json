{
  "name": "@tadashi/svelte-atom-label",
  "version": "1.0.0",
  "repository": "github:lagden/svelte-atom-label",
  "description": "Svelte component",
  "keywords": [
    "svelte",
    "label",
    "atom",
    "component"
  ],
  "svelte": "src/index.js",
  "main": "src/index.js",
  "type": "module",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "eslint": "eslint --config .eslintrc.cjs --ext .js --ignore-path .gitignore .",
    "rm": "rm -rf dist",
    "rollup": "rollup -c",
    "build": "run-s test rm rollup",
    "prepublishOnly": "npm run build",
    "pretest": "npm run eslint",
    "test": "jest test --coverage --coverageReporters=lcov --coverageReporters=text",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/eslint-parser": "7.14.3",
    "@babel/plugin-syntax-top-level-await": "7.12.13",
    "@babel/preset-env": "7.14.2",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@testing-library/svelte": "3.0.3",
    "eslint": "7.26.0",
    "eslint-config-xo": "0.36.0",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-svelte3": "3.2.0",
    "eslint-plugin-unicorn": "32.0.1",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "rollup": "2.48.0",
    "rollup-plugin-svelte": "7.1.0",
    "svelte": "3.38.2",
    "svelte-jester": "1.5.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-top-level-await"
    ]
  }
}
